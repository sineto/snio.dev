{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "checkJs": false,
    "allowJs": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "paths": {
      "@content/*": [
        "content/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    }
  },
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ]
}
